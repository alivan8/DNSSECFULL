zone "alcoymbra.com" {
           type master;
           file "/etc/bind/zones/db.alcoymbra.com"; #zone file path
      };
zone "168.192.in-addr.arpa" {
           type master;
           file "/etc/bind/zones/db.192.168";   #192.168.0.0/24 subnet
      };
